class Animal {
    void eat() {
        System.out.println("Animals can eat.");
    }
}
class Dog extends Animal {
    void bark() {
        System.out.println("Dog can bark.");
    }
}
public class SingleInheritanceDemo {
    public static void main(String[] args) {
        Dog myDog = new Dog();  
        myDog.eat();            
        myDog.bark();           
    }
}
